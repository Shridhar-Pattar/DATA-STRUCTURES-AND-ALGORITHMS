import java.util.Arrays;
public class Main{
 
    public static void main(String[] args) {
    int A[] = {1,2,3,4,5,6,7,8};
     
     reversetheArray(A,0,7);
System.out.println(Arrays.toString(A));


}
public static void reversetheArray(int A[] ,int start,int end){
        
    int temp;
    
while (start<end) {
    temp = A[start];
    A[start] = A[end];
    A[end] = temp;
    start++;
    end--;
}
    }
}
